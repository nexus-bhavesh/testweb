function send_email() {
    global $wpdb;

    $responseArray = array();

    if ($_POST['txt_name'] == '' || trim($_POST['txt_name']) == '') {
        $error = array();
        $error['name'] = 'txt_name';
        $errorArray[] = $error;
    }
    if ($_POST['txt_phoneno'] == '' || trim($_POST['txt_phoneno']) == '') {
        $error = array();
        $error['name'] = 'txt_phoneno';
        $errorArray[] = $error;
    }
    if ($_POST['txt_email'] == '' || trim($_POST['txt_email']) == '') {
        $error = array();
        $error['name'] = 'txt_email';
        $errorArray[] = $error;
    }
    
    if (count($errorArray) == 0) {
        
        echo json_encode(array("status" => 1, "message" => "Email has been sent to your email address."));
    } else {
        $responseArray['status'] = 0;
        $responseArray['error'] = $errorArray;
        echo json_encode($responseArray);
        exit;
    }
    die;
}

